{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=./database.sqlite"
  },
  "BackgroundServices": {
    "CameraHealthCheck": {
      "IntervalMinutes": 5
    },
    "StorageMonitoring": {
      "IntervalMinutes": 15
    },
    "DownloadJob": {
      "IntervalSeconds": 60,
      "MaxConcurrentDownloads": 50
    }
  },
  "AllowedHosts": "*"
}